<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
   <acl>
     <resources>
        <resource id="Magento_Backend::admin">
           <resource id="Caratlane_NetSuite::netsuite" title="NetSuite" sortOrder="10" >
              <resource id="Caratlane_NetSuite::monitoring" title="Monitoring" sortOrder="50" >
                 <resource id="Caratlane_NetSuite::log" title="Orders Logs" sortOrder="100" />
                 <resource id="Caratlane_NetSuite::logview" title="Debug Log Viewer" sortOrder="100" />
              </resource>
              <resource id="Caratlane_NetSuite::settings" title="Settings" sortOrder="150">
                 <resource id="Caratlane_NetSuite::config" title="Configuration" sortOrder="10" />
              </resource>
           </resource>
        </resource>
     </resources>
   </acl>
</config>