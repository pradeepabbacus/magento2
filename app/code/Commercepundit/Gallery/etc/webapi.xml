<?xml version="1.0"?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
     <!-- Get Gallery Images API -->
     <route url="/V1/store/getgalleryimages" method="GET">
        <service class="Commercepundit\Gallery\Api\GalleryManagerInterface" method="getGalleryImages"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>           
    </route>
</routes>
